{
  "name": "onedrive-b2-migration",
  "version": "1.0.0",
  "description": "OneDrive to Backblaze B2 Migration Web Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "build-client": "cd client && npm run build",
    "dev-client": "cd client && npm run dev",
    "worker": "node server/workers/migrationWorker.js",
    "dev-worker": "nodemon server/workers/migrationWorker.js",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d",
    "docker:stop:dev": "docker-compose -f docker-compose.dev.yml down",
    "docker:stop:prod": "docker-compose -f docker-compose.prod.yml down",
    "docker:logs:dev": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:logs:prod": "docker-compose -f docker-compose.prod.yml logs -f",
    "docker:build:dev": "docker-compose -f docker-compose.dev.yml build",
    "docker:build:prod": "docker-compose -f docker-compose.prod.yml build --no-cache"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "socket.io": "^4.7.2",
    "axios": "^1.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "crypto": "^1.0.1",
    "node-cron": "^3.0.2",
    "multer": "^1.4.5-lts.1",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "bull": "^4.12.0",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "onedrive",
    "backblaze",
    "b2",
    "migration",
    "rclone",
    "oauth"
  ],
  "author": "Migration App",
  "license": "MIT"
}
